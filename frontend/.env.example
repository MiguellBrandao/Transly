# API Configuration
VITE_API_URL=http://localhost:3001

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Video Compression (Frontend)
# Videos larger than this size (in MB) will be automatically compressed before upload
VITE_COMPRESSION_THRESHOLD_MB=100

# Compression quality settings (only applies to large videos)
# Resolution: 480, 640, 720, 1080 (height in pixels, width is auto)
# Video Bitrate: 150k, 200k, 300k, 500k (lower = smaller file, lower quality)
# Audio Bitrate: 64k, 96k, 128k
VITE_VIDEO_RESOLUTION=640
VITE_VIDEO_BITRATE=200k
VITE_AUDIO_BITRATE=96k
